<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER"
      prefHeight="212.0" prefWidth="272.0"
      spacing="10"
      xmlns:fx="http://javafx.com/fxml/1"
      xmlns="http://javafx.com/javafx/21">
   <Label text="Product Owner"/>
   <TableView prefHeight="149.0" prefWidth="350.0">
     <columns>
       <TableColumn prefWidth="100" text="User Story ID" fx:id="usid"/>
       <TableColumn prefWidth="90" text="Business Value" fx:id="bv"/>
        <TableColumn prefWidth="60" text="Add" fx:id="add"/>
     </columns>
   </TableView>
   <Button mnemonicParsing="false" prefHeight="17.0" prefWidth="142.0" text="Add to Product Backlog" />
</VBox>
